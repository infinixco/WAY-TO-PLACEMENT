.table-fill tr:hover td { background: rgb(2, 49, 139); color: rgb(255, 255, 255); }

.table-fill tr:first-child { border-top: none; }

.table-fill tr:nth-child(2n+1) td { background: rgb(221, 234, 248); }

.table-fill tr:nth-child(2n+1):hover td { background: rgb(2, 49, 139); }

.table-fill td { background: rgb(255, 255, 255); text-align: left; vertical-align: middle; font-size: 12px; border-right: 1px solid rgb(193, 195, 209); padding: 5px 10px; }

.table-fill td:last-child { border-right: 0px; }

.table-fill th.text-left { text-align: left; }

.table-fill th.text-center { text-align: center; }

.table-fill th.text-right { text-align: right; }

.table-fill td.text-left { text-align: left; }

.table-fill td.text-center { text-align: center; }

.table-fill td.text-right { text-align: right; }

