    "#program to know whether a file exists or not\n",
    "import os, sys\n",
    "fname=input('enter filename: ')\n",
    "\n",
    "if os.path.isfile(fname):\n",
    "    f=open(fname, 'r')\n",
    "else:\n",
    "    print(fname+ ' does not exist')\n",
    "    sys.exit()\n",
    "\n",
    "print('the file contents are:')\n",
    "str=f.read()\n",
    "print(str)\n",
    "f.close()"
   ]
  }
